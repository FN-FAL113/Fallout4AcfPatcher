<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <Authors>FN-FAL113</Authors>
    <ApplicationIcon>assets\vault-boy.ico</ApplicationIcon>
    <PackageProjectUrl>https://github.com/FN-FAL113/Fallout4AcfPatcher</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="assets\github.png" />
    <None Remove="assets\vault-boy.ico" />
    <None Remove="assets\vault-boy.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="assets\github.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="assets\vault-boy.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="assets\vault-boy.ico">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
